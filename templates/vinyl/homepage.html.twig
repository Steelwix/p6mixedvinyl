{% extends 'base.html.twig' %}
{% block title %}
	{{ parent() }}
	| Create a new record
{% endblock %}
{% block body %}
	<h1>{{ title }}</h1>

	<div>
		Tracks

		<ul>
			{% for track in tracks %}
				<li>{{track.artist|upper}}
					-
					{{track.song}}</li>
			{% endfor %}
		</ul>
	</div>
{% endblock %}
{#Twig Commentaires#}
